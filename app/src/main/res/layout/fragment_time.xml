<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context=".nav_fragments.core.TimeFragment">

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/choose_date_and_time" android:layout_gravity="center_horizontal"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/dateDoctor" android:layout_marginTop="20dp"
            android:textSize="18sp" android:layout_marginStart="30dp" android:padding="5dp"/>
    <ListView
            android:id="@+id/list_time"
            android:layout_width="match_parent"
            android:layout_height="match_parent" android:layout_marginTop="60dp"/>


</FrameLayout>